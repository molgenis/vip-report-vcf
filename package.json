{
  "name": "@molgenis/vip-report-vcf",
  "version": "2.0.3",
  "description": "TypeScript VCF library with support for both reading and writing",
  "scripts": {
    "build": "tsc --build",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/molgenis/vip-report-vcf.git"
  },
  "license": "LGPL-3.0",
  "lint-staged": {
    "src/**/*.ts": [
      "eslint",
      "prettier --write"
    ]
  },
  "bugs": {
    "url": "https://github.com/molgenis/vip-report-vcf/issues"
  },
  "homepage": "https://github.com/molgenis/vip-report-vcf#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vitest": "^2.0.5"
  }
}
